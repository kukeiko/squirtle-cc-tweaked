{
    "Lua.diagnostics.globals": [
        "turtle",
        "peripheral",
        "shell",
        "keys",
        "http",
        "fs",
        "module",
        "textutils",
        "redstone",
        "parallel",
        "gps",
        "bit",
        "window",
        "term",
        "rednet",
        "read",
        "colors",
        "pocket"
    ],
    "Lua.diagnostics.disable": [
        "lowercase-global",
        "undefined-field",
        "redefined-local",
    ],
    "vscode-lua-format.configPath": "vscode-lua-format.config",
    "Lua.completion.autoRequire": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": false
    }
}